<form action="#" method="post">
	<fieldset>
		<label for="name">Name:</label>
		<input type="text" id="name" placeholder="Enter yourfull name" />
		
		<label for="email">Email:</label>
		<input type="email" id="email" placeholder="Enteryour email address" />
		<label for="message">Message:</label>
		<textarea id="message" placeholder="What's on yourmind?">
		</textarea>
		
		<input type="submit" value="Send message" />
	
	</fieldset>
</form>